<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="925.0" styleClass="hbox-odd" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.introductiontose.controller.taikhoan.TaiKhoanController">
   <children>
      <Text fx:id="hoTen" strokeType="OUTSIDE" strokeWidth="0.0" text="Nguyễn Văn An" wrappingWidth="180.0">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin>
      </Text>
      <Text fx:id="ngaySinh" layoutX="25.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="01/01/2000" textAlignment="CENTER" wrappingWidth="120.0">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin>
      </Text>
      <Text fx:id="tenTaiKhoan" layoutX="66.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="an123456789" wrappingWidth="150.0">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin>
      </Text>
      <Text fx:id="soCccd" layoutX="92.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="001234567890" textAlignment="CENTER" wrappingWidth="100.0">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin>
      </Text>
      <Button fx:id="resetButton" alignment="CENTER" mnemonicParsing="false" onAction="#reset" styleClass="button-reset-account" text="RESET" textFill="#ff4343">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </HBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="deleteButton" alignment="CENTER" mnemonicParsing="false" onAction="#delete" styleClass="button-delete-account" text="Xóa tài khoản" textFill="#ff4343">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</HBox>
